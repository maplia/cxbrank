<tr>
  <th class="number"><%= music.number %></th>
  <td class="title text">
    <%= raw music.title %>
    <% if music.subtitle %>
      <small><%= raw music.subtitle %></small>
    <% end %>
  </td>
  <% DIFFICULTIES.each_value do |difficulty| %>
    <td class="<%= difficulty[:css_class] %> number">
      <%= sprintf_for_level(music.level(difficulty[:id])) %>
    </td>
    <td class="<%= difficulty[:css_class] %> number">
      <%= sprintf_for_notes(music.notes(difficulty[:id])) %>
    </td>
  <% end %>
</tr>
